		PEEKDATA	= 0x0002,
		POKEDATA	= 0x0005,
		CONT		= 0x0007,
		ATTACH		= 0x0010,
		GETREGS		= 0x000c,
		SETREGS		= 0x000d,
		DETACH		= 0x0011,
		GETREGSET	= 0x4204,
		SETREGSET	= 0x4205,
		SEIZE		= 0x4206,
		INTERRUPT	= 0x4207,
	}

